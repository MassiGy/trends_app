<% if(flashMessages && flashMessages.success){ %>
  <div style="width: 100%;height: 25px; display: flex; justify-content: center;align-items: center;">
    <h4 style="color: darkgreen; padding-top: 10px;">
      <%= flashMessages.success %>
    </h4>

  </div>


  <% } %>

    <% if(flashMessages && flashMessages.error){ %>

      <div style="width: 100%;height: 25px; display: flex; justify-content: center;align-items: center;">
        <h4 style="color: darkmagenta; padding-top: 10px;">
          <%= flashMessages.error %>
        </h4>

      </div>

      <% } %>
